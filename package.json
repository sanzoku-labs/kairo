{
  "name": "kairo",
  "version": "0.1.0",
  "description": "A functional, composable TypeScript library that eliminates glue code",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./extensions": {
      "types": "./dist/extensions.d.ts",
      "import": "./dist/extensions.mjs",
      "require": "./dist/extensions.js"
    },
    "./extensions/events": {
      "types": "./dist/extensions/events.d.ts",
      "import": "./dist/extensions/events.mjs",
      "require": "./dist/extensions/events.js"
    },
    "./extensions/transactions": {
      "types": "./dist/extensions/transactions.d.ts",
      "import": "./dist/extensions/transactions.mjs",
      "require": "./dist/extensions/transactions.js"
    },
    "./extensions/caching": {
      "types": "./dist/extensions/caching.d.ts",
      "import": "./dist/extensions/caching.mjs",
      "require": "./dist/extensions/caching.js"
    },
    "./extensions/plugins": {
      "types": "./dist/extensions/plugins.d.ts",
      "import": "./dist/extensions/plugins.mjs",
      "require": "./dist/extensions/plugins.js"
    },
    "./extensions/workflows": {
      "types": "./dist/extensions/workflows.d.ts",
      "import": "./dist/extensions/workflows.mjs",
      "require": "./dist/extensions/workflows.js"
    },
    "./extensions/performance": {
      "types": "./dist/extensions/performance.d.ts",
      "import": "./dist/extensions/performance.mjs",
      "require": "./dist/extensions/performance.js"
    },
    "./extensions/contracts": {
      "types": "./dist/extensions/contracts.d.ts",
      "import": "./dist/extensions/contracts.mjs",
      "require": "./dist/extensions/contracts.js"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "bun run tsup",
    "dev": "bun run tsup --watch",
    "test": "bun run vitest",
    "test:watch": "bun run vitest --watch",
    "test:coverage": "bun run vitest --coverage",
    "typecheck": "bun run tsc --noEmit",
    "lint": "bun run eslint .",
    "format": "bun run prettier --write .",
    "docs:dev": "NODE_ENV=development vitepress dev docs --open",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:api": "typedoc",
    "docs:api:single": "typedoc --options typedoc-single.json",
    "docs:api:onefile": "rm -rf temp-api-docs && typedoc --plugin typedoc-plugin-markdown --theme markdown --out temp-api-docs --name 'Kairo API Reference' && concat-md --toc --decrease-title-levels --dir-name-as-title temp-api-docs > kairo-api-reference.md && rm -rf temp-api-docs",
    "docs:api:watch": "typedoc --watch",
    "docs:all": "bun run docs:api && bun run docs:build"
  },
  "keywords": [
    "typescript",
    "functional",
    "composable",
    "pipeline",
    "result",
    "error-handling"
  ],
  "author": "Sovanaryth THORNG",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/coverage-v8": "^3.2.3",
    "concat-md": "^0.5.1",
    "eslint": "^9.29.0",
    "prettier": "^3.5.3",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.6.4",
    "typescript": "^5.8.3",
    "vitepress": "^1.6.3",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "zod": "^3.25.64"
  }
}
